                        <Border x:Name="SelectedInactiveBorder" BorderThickness="1,1,1,1" CornerRadius="1,1,1,1" Padding="0,0,0,0" BorderBrush="{DynamicResource NormalBorderBrush}" Opacity="0" IsHitTestVisible="False">
